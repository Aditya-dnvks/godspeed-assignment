{
  "name": "scaffoldinggodspeed",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cd gs_service && npm run test",
    "test-cov": "cd gs_service && npm run test-cov",
    "dev": "PORT=3000 node -r ./gs_service/dist/core/logger.js -r ./gs_service/dist/telemetry/tracing.js gs_service/dist/app.js | pino-pretty",
    "clean": "cd gs_service && npm run clean",
    "build": "mkdir -p  gs_service/project && && for f in src/datasources/*.prisma; do npx prisma generate --schema=$f; done && rsync --delete -rv src config gs_service/project/ && npm i && cd gs_service && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.6.1",
    "prisma": "^3.14.0"
  },
  "dependencies": {
    "@opentelemetry/tracing": "^0.24.0",
    "@prisma/client": "^3.14.0"
  }
}
